# Frontend Environment Variables - Production (Render Deployment)
# API Configuration - Will use same domain as frontend
REACT_APP_API_URL=
REACT_APP_BACKEND_URL=

# Firebase Configuration (Frontend) - Get from Firebase Console
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Payment Configuration - Use production keys for live deployment
REACT_APP_RAZORPAY_KEY_ID=rzp_live_your_production_key

# Environment
REACT_APP_NODE_ENV=production